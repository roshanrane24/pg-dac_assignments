<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>cms-jsp</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>db_url</param-name>
    <param-value>jdbc:mysql://localhost:3306/j2ee?useSSL=false&amp;allowPublicKeyRetrieval=true</param-value>
  </context-param>
  <context-param>
    <param-name>db_user</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>db_pass</param-name>
    <param-value>j2ee_root</param-value>
  </context-param>
  <error-page>
    <exception-type>Throwable</exception-type>
    <location>/error_handler.jsp</location>
  </error-page>
</web-app>